{
  "name": "ucur-app-api",
  "version": "1.0.0",
  "description": "A backend for Ucur Application that uses Node.js, Socket.io, Express.js, MongoDB",
  "main": "app.js",
  "scripts": {
    "dev": "node app.js",
    "start": "nodemon app.js --exec babel-node --presets es2015,stage-2",
    "build": "babel lib -d dist",
    "serve": "node dist/app.js",
    "test": "mocha --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/canozgen9/ucur-app-api.git"
  },
  "keywords": [
    "ucur",
    "api",
    "mongodb",
    "nodejs",
    "expressjs",
    "socketio"
  ],
  "author": "Vertex",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/canozgen9/ucur-app-api/issues"
  },
  "homepage": "https://github.com/canozgen9/ucur-app-api#readme",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.1",
    "mongoose": "^5.0.6",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "socketio": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.11.0",
    "babel-register": "^6.11.6"
